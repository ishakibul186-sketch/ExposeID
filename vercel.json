{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/index.html"
    },
    {
      "source": "/sitemap-userprofile.xml",
      "destination": "/index.html"
    },
    {
      "source": "/docs",
      "destination": "/index.html"
    },
    {
      "source": "/terms",
      "destination": "/index.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
